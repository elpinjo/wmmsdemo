<ehcache     
    name="MachineClients">    
    <diskStore         
        path="./cacheStore/MachineClients"/>    
    <defaultCache         
        maxEntriesLocalHeap="0"/>    
    <cache         
        name="clientlist"         
        eternal="true"         
        maxEntriesLocalHeap="10000"         
        overflowToOffHeap="false"         
        cacheLoaderTimeoutMillis="30000">        
        <persistence             
            strategy="localtempswap"/>    
    </cache>
</ehcache>
